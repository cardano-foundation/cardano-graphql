    "glob-parent": "5.1.2",
    "got": "11.8.5",
    "json5": "2.2.2",
    "jsonwebtoken": "9.0.0",
    "minimatch": "3.0.5",
    "node-fetch": "2.6.7",
    "node-notifier": "8.0.1",
    "semver": "5.7.2",
    "tough-cookie": "4.1.3",
    "word-wrap": "1.2.4",
    "ws": "7.4.6"
  },
  "dependencies": {}
}
